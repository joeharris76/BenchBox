   "source": [
    "# Head-to-Head Comparison\n",
    "\n",
    "Generate side-by-side charts for two platforms. Swap the synthetic data with real result exports or pass result\n+    "files to `benchbox visualize --template head_to_head`."
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "from datetime import datetime\n",
    "from benchbox.core.visualization import NormalizedQuery, NormalizedResult, ResultPlotter\n",
    "\n",
    "\n",
    "def build_result(platform, total_ms, avg_ms, queries):\n",
    "    return NormalizedResult(\n",
    "        benchmark=\"TPC-DS\",\n",
    "        platform=platform,\n",
    "        scale_factor=0.1,\n",
    "        execution_id=f\"{platform}-demo\",\n",
    "        timestamp=datetime(2025, 2, 1),\n",
    "        total_time_ms=total_ms,\n",
    "        avg_time_ms=avg_ms,\n",
    "        success_rate=1.0,\n",
    "        cost_total=None,\n",
    "        queries=[NormalizedQuery(query_id=qid, execution_time_ms=ms, status=\"SUCCESS\") for qid, ms in queries],\n",
    "    )\n",
    "\n",
    "\n",
    "duckdb_queries = [(\"Q3\", 380), (\"Q7\", 410), (\"Q12\", 440), (\"Q25\", 390)]\n",
    "snowflake_queries = [(\"Q3\", 320), (\"Q7\", 350), (\"Q12\", 370), (\"Q25\", 345)]\n",
    "\n",
    "results = [\n",
    "    build_result(\"duckdb\", total_ms=10_500, avg_ms=350, queries=duckdb_queries),\n",
    "    build_result(\"snowflake\", total_ms=9_200, avg_ms=310, queries=snowflake_queries),\n",
    "]\n",
    "\n",
    "plotter = ResultPlotter(results)\n",
    "plotter.generate_all_charts(\n",
    "    output_dir=\"charts/head-to-head\",\n",
    "    template_name=\"head_to_head\",\n",
    "    formats=(\"png\", \"html\"),\n",
    ")\n"
   ]
  }
 ],
